<?xml version="1.0" encoding="UTF-8"?>
<api context="/produce" name="Produce_API" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST">
        <inSequence>
            <property expression="json-eval($.priority)" name="priority" scope="default" type="STRING"/>
            <property expression="json-eval($.message)" name="message" scope="default" type="STRING"/>
            <log level="custom">
                <property expression="$ctx:priority" name="priority"/>
                <property expression="json-eval($.message)" name="message"/>
            </log>
            <class name="com.uol.knot.ProduceRabbit"/>
            <log level="custom">
                <property name="afetr" value="after class"/>
            </log>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </resource>
</api>
